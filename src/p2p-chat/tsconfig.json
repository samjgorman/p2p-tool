{
  "compilerOptions": {
    "allowJs": true,
    "module": "commonjs",
    "jsx":"react",
    "skipLibCheck": true,
    "esModuleInterop": true,
    // "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "paths": {
      "*": ["node_modules/*"]
    },
  },
  // "include": ["client/**/*", "electron/**/*", "server/**/*", "test/**/*", ],
  "include": ["src/**/*", "electron/**/*", "server/**/*", "test/**/*", ],

  "typedocOptions": {
    "entryPointStrategy": "expand",
    "entryPoints": ["./src"],
    "out": "docs",

}
}
